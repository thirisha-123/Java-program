import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
       Square obj=new Square();
        int n=sc.nextInt();
        if(n>0 && n<9)
        {
            System.out.println("Square of "+n+" is "+obj.Square(n));
        }
        else
        {
            System.out.println("Invalid Input");
        }
    }
}
class Square{
        int Square(int n){
            return(n*n);
        }
    }
