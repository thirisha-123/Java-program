import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        String S=sc.nextLine();
        char arr[]=S.toCharArray();
        for(int i=0;i<arr.length;i++){
            if(arr[i]==' '){
                continue;
            }
            else{
                System.out.print(arr[i]);
            }
        }
    }
}
