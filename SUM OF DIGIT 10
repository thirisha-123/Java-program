import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
        int num=sc.nextInt();
        int sum=0;
        if(num<100){
          System.out.println("Invalid Input");   
        }
        else{
          while(num>0){
              int rem=num%10;
              sum=sum+rem;
              num=num/10;
          }  
            System.out.println("Sum of digit is "+sum);
        }
    }
}
