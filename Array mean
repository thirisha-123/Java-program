import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int size=sc.nextInt();
        int sum=0; double avg=0.0f;
        int arr[]=new int[size];
        for(int i=0;i<size;i++)
            {
            arr[i]=sc.nextInt();
            sum+=arr[i];
            }
        avg=(float)sum/size;
        System.out.printf("Array Mean Value is %.2f",(float)avg);
    }
}
